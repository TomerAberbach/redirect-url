{
  "name": "redirect-url",
  "version": "1.2.1",
  "author": {
    "name": "Tomer Aberbach",
    "email": "tomer@aberba.ch",
    "url": "https://tomeraberba.ch"
  },
  "description": "Simple rule-based redirecting from one URL to another.",
  "keywords": [
    "http",
    "https",
    "mapping",
    "redirect",
    "path",
    "server",
    "url"
  ],
  "homepage": "https://github.com/TomerAberbach/redirect-url",
  "repository": "TomerAberbach/redirect-url",
  "bugs": {
    "url": "https://github.com/TomerAberbach/redirect-url/issues"
  },
  "funding": {
    "url": "https://github.com/sponsors/TomerAberbach"
  },
  "license": "Apache-2.0 AND MIT",
  "files": [
    "dist",
    "license-apache",
    "license-mit",
    "notice-apache"
  ],
  "type": "module",
  "sideEffects": false,
  "engines": {
    "node": ">= 22"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "format": "prettier --cache --write .",
    "lint": "eslint --cache --cache-location node_modules/.cache/eslint/ --fix .",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "coverage": "vitest --coverage",
    "bench": "vitest bench",
    "build": "tsdown",
    "prepublishOnly": "pnpm build",
    "docs": "typedoc --plugin typedoc-plugin-markdown --githubPages false"
  },
  "prettier": "@tomer/prettier-config",
  "dependencies": {
    "path-to-regexp": "^6.3.0",
    "type-fest": "^4.41.0"
  },
  "devDependencies": {
    "@fast-check/vitest": "^0.2.2",
    "@rollup/plugin-terser": "^0.4.4",
    "@tomer/eslint-config": "^4.1.1",
    "@tomer/prettier-config": "^4.0.0",
    "@types/node": "^24.3.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.34.0",
    "jest-extended": "^6.0.0",
    "prettier": "^3.6.2",
    "publint": "^0.3.12",
    "rollup-plugin-tree-shakeable": "^2.0.0",
    "tsdown": "^0.14.2",
    "typedoc": "^0.28.12",
    "typedoc-plugin-markdown": "^4.8.1",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
